{
  "name": "frontpage",
  "version": "0.0.1",
  "description": "wangdoc.com's frontpage",
  "main": "index.js",
  "scripts": {
    "build": "loppo -t wangdoc-frontpage",
    "build-and-commit": "npm run build && npm run commit",
    "server": "loppo server",
    "commit": "gh-pages --dist dist --dest dist --branch master --add --repo git@github.com:wangdoc/website.git",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Wangdoc",
    "document"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wangdoc/frontpage.git"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm update"
    }
  },
  "author": "Ruan Yifeng",
  "license": "ISC",
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "husky": "^4.3.8",
    "loppo": "^0.6.24",
    "loppo-theme-wangdoc-frontpage": "^0.4.1"
  }
}
